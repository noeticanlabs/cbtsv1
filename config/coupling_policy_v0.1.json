  "error_codes": {
    "ERR_NAN_INF": "abort_simulation",
    "ERR_EPS_H_VIOLATION": "retry_with_dt_reduction",
    "ERR_EPS_M_VIOLATION": "retry_with_dt_reduction",
    "ERR_R_MIN_VIOLATION": "abort_simulation",
    "ERR_ENERGY_DRIFT": "apply_dissipation",
    "ERR_CFL_VIOLATION": "retry_with_dt_reduction"
  },
  "ops_allowed_to_touch_S_PHY": [
    "projection",
